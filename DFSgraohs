#include <iostream>
#include <vector>
using namespace std;

vector<vector<int>> g;   // graph

void dfsUtil(int v, vector<bool>& vis) {
    vis[v] = true;
    cout << v << " ";

    for (int x : g[v]) {
        if (!vis[x]) {
            dfsUtil(x, vis);
        }
    }
}

void DFS(int u, int V) {
    vector<bool> vis(V, false);
    dfsUtil(u, vis);
}

int main() {
    int V = 5;
    g.resize(V);

    g[0].push_back(1);
    g[0].push_back(2);
    g[1].push_back(3);
    g[1].push_back(4);

    DFS(0, V);
    return 0;
}
